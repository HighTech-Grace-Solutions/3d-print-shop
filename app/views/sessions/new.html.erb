<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %> 
  <%= f.label :username =%></br>
  <%= f.text_field :username, :placeholder => "Username" %></br></br>
  <%= f.label :password =%></br>
  <%= f.password_field :password, :placeholder => "Password" %></br></br>
  <%= f.submit "Log in", class: "btn-submit" %>
<% end %>
